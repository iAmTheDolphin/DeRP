function main using () {

	list x [10]

	x[0] = 1500

	print("x[0] is : " + x[0])
	print("moving yeet in to x[9]")

	x[9] = "yeet"

	print(x[9])

	print("--------------------------------")
	z = 9

	print("loading x[] with a loop")
	loop while z > 0 {
		print(z)
		x[z] = z
		z = z - 1
	}
	lambda using () {}
	z = 9
	print("printing x[] with a loop")
    loop while z > 0 {
    	print(x[z])
    	z = z - 1
    }
}